{
    "name": "selection-info",
    "displayName": "selection-info",
    "description": "",
    "version": "0.0.1",
    "publisher": "ega",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.getSelectionInfo"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.getSelectionInfo",
                "title": "Get selection info"
            }
        ],
        "menus": {
            "editor/context": [{
                "command": "extension.getSelectionInfo",
                "when": "editorHasSelection",
                "group": "navigation"
            }]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6"
    },
    "dependencies": {
        "copy-paste": "^1.3.0"
    }
}
