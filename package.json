{
    "name": "quick-copy-as-markdown",
    "displayName": "quick-copy-as-markdown",
    "description": "",
    "version": "0.18.0",
    "publisher": "akiega",
    "license": "MIT",
    "repository": "https://github.com/AkiEga/vscode-extension-section-info.git",
    "engines": {
        "vscode": "^1.47.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.QuickCopyAsMd.copyToClipboard",
        "onCommand:extension.QuickCopyAsMd.timeLineTrace"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.QuickCopyAsMd.copyToClipboard",
                "title": "Copy selected code as markdown"
            },
            {
                "command": "extension.QuickCopyAsMd.enableTraceMode",
                "title": "Enable a mode to trace code reading actions"
            },
            {
                "command": "extension.QuickCopyAsMd.quickMarkInTraceMode",
                "title": "Enable a mode to trace code reading actions"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "command": "extension.QuickCopyAsMd.copyToClipboard",
                    "when": "editorHasSelection",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "vscode:publish": "vsce publish minor -p `cat ./PackageAccessToken.txt` --yarn",
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "yarn compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "typescript": "^2.9.2",
        "vscode": "^1.1.18"
    },
    "dependencies": {
        "copy-paste": "^1.3.0",
        "es6-template-strings": "^2.0.1",
        "npm": "^6.14.6"
    }
}
